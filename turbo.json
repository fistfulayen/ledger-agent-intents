{
	"$schema": "https://turbo.build/schema.json",
	"globalPassThroughEnv": [
		"POSTGRES_URL",
		"POSTGRES_URL_NON_POOLING",
		"POSTGRES_PRISMA_URL",
		"POSTGRES_USER",
		"POSTGRES_HOST",
		"POSTGRES_PASSWORD",
		"POSTGRES_DATABASE",
		"PGHOST",
		"PGHOST_UNPOOLED",
		"PGUSER",
		"PGPASSWORD",
		"PGDATABASE",
		"PGPORT",
		"PGUSER",
		"DATABASE_URL",
		"NEON_PROJECT_ID",
		"VITE_LEDGER_API_KEY",
		"VITE_BACKEND_URL"
	],
	"tasks": {
		"build:styles": {
			"outputs": ["dist/styles.css"]
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".output/**", ".vinxi/**"]
		},
		"dev": {
			"dependsOn": ["^build:styles"],
			"cache": false,
			"persistent": true
		},
		"typecheck": {
			"dependsOn": ["^build"]
		},
		"clean": {
			"cache": false
		}
	}
}
